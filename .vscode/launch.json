
{
    "version": "0.1.0",
    "configurations": [
        {
            "name": "1",
            "request": "launch",
            "type": "pwa-chrome",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}"
        },
        {
            "name": "Attach to Chrome",
            "port": 9222,
            "request": "attach",
            "type": "pwa-chrome",
            "file": "${workspaceRoot}/index.html",
            "webRoot": "${workspaceFolder}"
        }
    ]
}
